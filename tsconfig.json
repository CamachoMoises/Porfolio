{
  "compileOnSave": false,
  "compilerOptions": {
    "paths": {
      "@app/*": ["src/app/*"],
      "@shared/*": ["src/app/shared/*"],
      "@characters/*": ["src/app/components/pages/characters0/*"],
      "@pages/*": ["src/app/components/pages/*"],
      "@environments/*": ["src/environments/*"],
    },
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "baseUrl": "src",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "types": [
      "node",
      "jest"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "module": "esnext",
    "allowSyntheticDefaultImports": true
  },
  "angularCompilerOptions": {
    "strictTemplates": false
  }
}